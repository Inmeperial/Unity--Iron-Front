			o.Emission = ( _ColorAttack1 * temp_output_3_0 ).rgb;
			o.Alpha = ( temp_output_3_0 * _Opacity1 ).r;
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf Unlit alpha:fade keepalpha fullforwardshadows 

		ENDCG
