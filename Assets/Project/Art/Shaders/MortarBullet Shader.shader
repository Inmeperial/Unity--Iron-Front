		}

		void surf( Input i , inout SurfaceOutput o )
		{
			float mulTime5 = _Time.y * 2.0;
			float2 panner4 = ( mulTime5 * float2( 0,-1 ) + float2( 0,0 ));
			float2 uv_TexCoord1 = i.uv_texcoord * float2( 0,1 ) + panner4;
